# Extract directory name from full path of the file currently being processed.
# Note that CMake 2.8.3 introduced CMAKE_CURRENT_LIST_DIR. We reimplement it
# for older versions of CMake to support these as well.
if(CMAKE_VERSION VERSION_LESS "2.8.3")
  get_filename_component(CMAKE_CURRENT_LIST_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
endif()

if(NOT DEFINED Irrlicht_CONFIG_SUBDIR)
  set(Irrlicht_CONFIG_SUBDIR "/abi-${ANDROID_NDK_ABI_NAME}")
endif()

set(Irrlicht_CONFIG_PATH "${CMAKE_CURRENT_LIST_DIR}${Irrlicht_CONFIG_SUBDIR}")
if(EXISTS "${Irrlicht_CONFIG_PATH}/IrrlichtConfig.cmake")
  include("${Irrlicht_CONFIG_PATH}/IrrlichtConfig.cmake")
else()
  if(NOT Irrlicht_FIND_QUIETLY)
    message(WARNING "Found Irrlicht Android Pack but it has no binaries compatible with your ABI (can't find: ${Irrlicht_CONFIG_SUBDIR})")
  endif()
  set(Irrlicht_FOUND FALSE)
endif()
